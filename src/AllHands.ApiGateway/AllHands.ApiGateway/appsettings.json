{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "timeoff-route": {
        "ClusterId": "timeoff-cluster",
        "Match": {
          "Path": "/api/v{version:apiVersion}/time-off/{**catch-all}"
        }
      },
      "holidays-route": {
        "ClusterId": "timeoff-cluster",
        "Match": {
          "Path": "/api/v{version:apiVersion}/holidays/{**catch-all}"
        }
      },
      "news-route": {
        "ClusterId": "news-cluster",
        "Match": {
          "Path": "/api/v{version:apiVersion}/news/{**catch-all}"
        }
      },
      "permissions-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/v{version:apiVersion}/permissions/{**catch-all}"
        }
      },
      "roles-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/v{version:apiVersion}/roles/{**catch-all}"
        }
      },
      "positions-route": {
        "ClusterId": "employees-cluster",
        "Match": {
          "Path": "/api/v{version:apiVersion}/positions/{**catch-all}"
        }
      },
      "company-route": {
        "ClusterId": "employees-cluster",
        "Match": {
          "Path": "/api/v{version:apiVersion}/company/{**catch-all}"
        }
      },
      "employees-timeoff-route": {
        "Order": 0,
        "ClusterId": "timeoff-cluster",
        "Match": {
          "Path": "/api/v{version:apiVersion}/employees/time-off/{**catch-all}"
        }
      },
      "employees-resend-invitation-route": {
        "Order": 1,
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/v{version:apiVersion}/employees/{employeeId:guid}/invitations/{**catch-all}"
        }
      },
      "employees-route": {
        "Order": 2,
        "ClusterId": "employees-cluster",
        "Match": {
          "Path": "/api/v{version:apiVersion}/employees/{**catch-all}"
        }
      },
      "account-login": {
        "Order": 0,
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/v{version:int}/account/login"
        }
      },
      "account-logout": {
        "Order": 1,
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/v{version:int}/account/logout"
        }
      },
      "account-relogin": {
        "Order": 2,
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/v{version:int}/account/relogin"
        }
      },
      "account-register-invitation": {
        "Order": 3,
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/v{version:int}/account/register/invitation"
        }
      },
      "account-reset-password": {
        "Order": 4,
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/v{version:int}/account/reset-password"
        }
      },
      "account-password": {
        "Order": 5,
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/v{version:int}/account/password"
        }
      },
      "account-employee": {
        "Order": 100,
        "ClusterId": "employees-cluster",
        "Match": {
          "Path": "/api/v{version:int}/account/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "Destination1": {
            "Address": ""
          }
        }
      },
      "employees-cluster": {
        "Destinations": {
          "Destination1": {
            "Address": ""
          }
        }
      },
      "timeoff-cluster": {
        "Destinations": {
          "Destination1": {
            "Address": ""
          }
        }
      },
      "news-cluster": {
        "Destinations": {
          "Destination1": {
            "Address": ""
          }
        }
      }
    }
  }
}
