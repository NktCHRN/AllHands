{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "HttpApi": {
        "Url": "http://0.0.0.0:5000",
        "Protocols": "Http1"
      },
      "Grpc": {
        "Url": "http://0.0.0.0:5001",
        "Protocols": "Http2"
      }
    }
  },
  "ConnectionStrings": {
    "postgres": ""
  },
  "AllowedOrigins": [  ],
  "AWS": {
    "Region": "eu-central-1"
  },
  "S3": {
    "BucketName": "allhands-prod-1",
    "AvatarsPrefix": "avatars",
    "DefaultAvatarName": "default.jpg",
    "CompanyLogosPrefix": "companies/logos",
    "DefaultCompanyLogoName": "default.png"
  },
  "Marten": {
    "MaxRetryAttempts": 3,
    "Delay": "00:00:00.500"
  },
  "UserRoleUpdaterOptions": {
    "BatchSize": 1000
  },
  "AuthService": {
    "grpc": ""
  }
}
