{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "HttpApi": {
        "Url": "http://0.0.0.0:5000",
        "Protocols": "Http1"
      },
      "Grpc": {
        "Url": "http://0.0.0.0:5001",
        "Protocols": "Http2"
      }
    }
  },
  "ConnectionStrings": {
    "redis": "",
    "postgres": ""
  },
  "InvitationTokenProviderOptions": {
    "LifeTimeInMinutes": 1440,
    "TokenRecreationTimeoutInSeconds": 60
  },
  "PasswordResetTokenProviderOptions": {
    "LifeTime": "24:00:00",
    "TokenRecreationTimeout": "00:01:00"
  },
  "AllowedOrigins": [  ],
  "CookieAuthenticationOptions": {
    "ExpireTimeSpan": "00:30:00",
    "SlidingExpiration": true,
    "Cookie": {
      "Name": "AllHands.Auth",
      "SameSite": "Lax",
      "HttpOnly": true,
      "SecurePolicy": "Always"
    }
  },
  "AWS": {
    "Region": "eu-central-1"
  },
  "EmailSenderOptions": {
    "FromEmailAddress": "",
    "ResetPasswordUrl": "",
    "ResetPasswordTemplateName": "",
    "CompleteRegistrationUrl": "",
    "CompleteRegistrationTemplateName": ""
  },
  "SessionRecalculatorOptions": {
    "BatchSize": 1000
  },
  "Marten":
  {
    "MaxRetryAttempts": 3,
    "Delay": "00:00:00.500"
  }
}
